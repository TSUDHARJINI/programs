import java.util.*;
public class Main
{
	public static void main(String[] args)
  {
  int a;
		System.out.println("Enter a number");
		Scanner sn=new Scanner(System.in);
		 a=sn.nextInt();
        if(a%2==0)
          System.out.println(a +" is even number");
        else
          System.out.println(a +" is odd number");
	}
}
