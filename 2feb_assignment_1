#include <stdio.h>    
int main()    
{    
    int arr[50];  
    int n,temp = 0;    
  printf("enter how many elements");
  scanf("%d",&n);
  for(int i=0;i<n;i++)
  {
   scanf("%d",&arr[i]);
  }
    printf("Elements of original array: \n");    
    for (int i = 0; i <n; i++) {     
    printf("%d ", arr[i]);     
    }      
    for (int i = 0; i < n; i++)
 {     
    for (int j = i+1; j < n; j++) 
   {     
    if(arr[i] > arr[j]) 
    {    
    temp = arr[i];    
    arr[i] = arr[j];    
    arr[j] = temp;    
     }      
    }     
  }    
    printf("\n");    
    printf("Elements of array sorted in ascending order: \n");    
    for (int i = 0; i < n; i++) {     
    printf("%d ", arr[i]);    
    }    
    return 0;    
}     


