import java.util.*;
public class Main
{
	public static void main(String[] args)
	{
	    int n,s=0,i=1;
		System.out.println("Enter a number");
		Scanner sn=new Scanner(System.in);
		n=sn.nextInt();
		while(i<=n)
		{
	       s=s+i;
		   i=i+1;
		}
		    System.out.println("Sum of 1 to " +n+" numbers is  "+s);
		 
	}
}
