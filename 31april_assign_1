import java.util.*;
class Main
{  
 public static void main(String args[])
 {  
  int i,fact=1,number;
  Scanner sn=new Scanner(System.in);
  System.out.println("Enter a value");
  number=sn.nextInt();
  for(i=1;i<=number;i++)
        fact=fact*i;    
  System.out.println("Factorial of "+number+" is: "+fact);    
 }  
}  
