import java.util.*;
class Main
{
public static void main (String[] args)
{
  Scanner sn= new Scanner(System.in);
       String s1,s2;
       System.out.println("enter first string");
       s1=sn.nextLine();
       System.out.println("enter second string");
       s2=sn.nextLine();
       
       if(s1.equals(s2))
       System.out.println("both the strings r same");
       else
       System.out.println("both the strings r not same");
       
       if(s1.contains(s2))
        System.out.println("yes...the given string is substring of first");
    else
    
    System.out.println("no...the given string is not substring of first");
    }
}
